  "license": "ISC",
  "homepage": "https://github.com/Saifadin/gatsby-plugin-playground#readme",
  "repository": "https://github.com/Saifadin/gatsby-plugin-playground",
  "bugs": {
    "url": "https://github.com/Saifadin/gatsby-plugin-playground/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "graphql-playground-middleware-express": "^1.7.8",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
